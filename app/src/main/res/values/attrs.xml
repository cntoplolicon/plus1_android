<resources>
    <declare-styleable name="ActionBarLayout">
        <attr name="page_title" format="string" />
        <attr name="hide_back_arrow" format="boolean" />
    </declare-styleable>
    <declare-styleable name="AutoHeightScrollView">
        <attr name="maxHeight" format="dimension" />
    </declare-styleable>
    <declare-styleable name="CustomNicknameLayout">
        <attr name="hide_gender_icon" format="boolean" />
        <attr name="linked_to_user_home" format="boolean" />
    </declare-styleable>
    <declare-styleable name="UserAvatarImageView">
        <attr name="link_to_user_home" format="boolean" />
    </declare-styleable>
    <declare-styleable name="AdjustableImageView">
        <attr name="min_height_width_ratio" format="float" />
        <attr name="max_height_width_ratio" format="float" />
    </declare-styleable>
</resources>