<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@color/white"
    tools:context="swj.swj.activity.ResetPhoneStepTwoActivity">

    <include
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        layout="@layout/activity_verify_security_code"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/fragment5" />

    <fragment
        android:layout_width="match_parent"
        android:layout_height="52dp"
        android:name="swj.swj.fragment.CustomActionBarFragment"
        android:id="@+id/fragment5"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        tools:layout="@layout/fragment_custom_action_bar" />


</RelativeLayout>
